Program lendo_numeros ;
var
	primeiro_numero, segundo_numero, terceiro_numero :integer;
	
Begin
	writeln('Digite o primeiro numero: ');
	readln(primeiro_numero);
	
	writeln('Digite o segundo numero: ');
	readln(segundo_numero);
	
	writeln('Digite o terceiro numero: ');
	readln(terceiro_numero);
	
  writeln('Os Números digitados foram: ', primeiro_numero ,
	' ', segundo_numero, ' ', terceiro_numero);	 
End.